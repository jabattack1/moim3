		var valueFirstname = $('#firstnameSignup').val();
		var valueLastname = $('#lastnameSignup').val();
		var valueMonth = $('#monthSignup').val();
		var valueDay = $('#daySignup').val();
		var valueYear = $('#yearSignup').val();
		var valueFemale = $('#female').val();
		var valueMale = $('#male').val();

        var result = email(value);


// *****required*****
        if (result == false) {
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#emailRequired').show();
        } 

		if (valueUsername.length == 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#usernameRequired').show();
        }

        if (valuePassword.length < 8){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#passwordRequired').show();
        }

        if (valueFirstname.length == 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#firstnameRequired').show();
        }
        
        if (valueLastname.length == 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#lastnameRequired').show();
        }

        if (valueMonth.length < 2){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#birthdayRequired').show();
        }
        
        if (valueDay.length < 2){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#birthdayRequired').show();
        }
        
        if (valueYear.length < 4){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#birthdayRequired').show();
        }

        if ($('#male').prop('checked') == false && $('#female').prop('checked') == false ){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#genderRequired').show();
        }

// ****remove required****

        if (result == true) {
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#emailRequired').hide();
        } 

		if (valueUsername.length != 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#usernameRequired').hide();
        }

        if (valuePassword.length > 7){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#passwordRequired').hide();
        }

        if (valueFirstname.length != 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#firstnameRequired').hide();
        }

        if (valueLastname.length != 0){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#lastnameRequired').hide();
        }

        if (valueYear.length > 3 && valueDay.length > 1 && valueMonth.length > 1){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#birthdayRequired').hide();
        }

        if ($('#male').prop('checked') == true || $('#female').prop('checked') == true ){
            $('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);
			$('#genderRequired').hide();
        }


        if (result == true && valueUsername.length != 0 && valuePassword.length > 7 && valueFirstname.length != 0 && valueLastname.length != 0 && valueYear.length > 3 && valueDay.length > 1 && valueMonth.length > 1 && $('#male').prop('checked') == true){
            $('button[id="signupButton"]').prop("disabled", false);
        }

        else if (result == true && valueUsername.length != 0 && valuePassword.length > 7 && valueFirstname.length != 0 && valueLastname.length != 0 && valueYear.length > 3 && valueDay.length > 1 && valueMonth.length > 1 && $('#female').prop('checked') == true){
            $('button[id="signupButton"]').prop("disabled", false);
        }
        else {
        	$('button[id="signupButton"]').prop("disabled", true);
            setTimeout(function() {
  				$('button[id="signupButton"]').prop("disabled", false);
			}, 300);

        }


});
